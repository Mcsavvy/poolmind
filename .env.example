# Example environment variables for PoolMind
# Copy this file to .env and fill in your actual values

# API Configuration
API_URL=https://poolmind.futurdevs.com/api

# Exchange API Keys
# Hotcoin
HOTCOIN_ACCESS_KEY=your_hotcoin_access_key
HOTCOIN_SECRET_KEY=your_hotcoin_secret_key

# Gate.io
GATE_API_KEY=your_gate_api_key
GATE_API_SECRET=your_gate_api_secret

# Binance
BINANCE_API_KEY=your_binance_api_key
BINANCE_API_SECRET=your_binance_api_secret

# Coin W
COINW_API_KEY=your_coinw_api_key
COINW_SECRET_KEY=your_coinw_secret_key

# Bybit
BYBIT_API_KEY=your_bybit_api_key
BYBIT_API_SECRET=your_bybit_api_secret

# OrangeX
ORANGEX_API_KEY=your_orangex_api_key
ORANGEX_SECRET_KEY=your_orangex_secret_key
